/* Space out content a bit */
body
  overflow: hidden
  position: absolute
  left: 0
  right: 0
  bottom: 0
  top: 0
  font-family: 'Roboto Slab', sans-serif
  background-color: #F1F2F1
  color: #000 !important
.wrapper
  overflow: hidden
  overflow-y: scroll
  -webkit-overflow-scrolling: touch
  float: left
  width: 100%
  height: 500px
  top: 50px
  left: 0
  right: 0
  bottom: 0
  padding: 10px 0 10px 0
  margin-top: 70px
.wrapper-timeline
  background: white
.wrapper_titre_timeline
